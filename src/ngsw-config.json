{
    "index": "/index.html",
    "start_url": "/",
    "appData": {
        "version": "2.0.0",
        "changelog": "PWA integration has been completed."
    },
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.html",
                    "/*.css",
                    "/*.js"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/assets/**",
                    "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
                ]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "rates",
            "urls": [
                "https://api.exchangeratesapi.io/latest?base=USD"
            ],
            "cacheConfig": {
                "maxSize": 10,
                "maxAge": "1d",
                "timeout": "10s",
                "strategy": "freshness"
            }
        }
    ]
}
